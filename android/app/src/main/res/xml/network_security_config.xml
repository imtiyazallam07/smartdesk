<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config>
        <cleartext-traffic permitted="true" />
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
    <domain-config>
        <domain includeSubdomains="true">soaportals.com</domain>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
            </trust-anchors>
        </domain-config>
</network-security-config>